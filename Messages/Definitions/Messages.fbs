namespace MessagesX;

table Ping
{
  source:int;
}

table BroadcastPing
{
    source_address:string;
}

table Abort
{
  note:string;
}


table RegisterUserEquipment
{
  id:int;
  pool_address:string;
}

table Log
{
  note:string;
}

union Payload
{
  Ping,
  Abort,
  RegisterUserEquipment,
  Log
}

table Message
{
  payload:Payload;  
}

root_type Message; 
